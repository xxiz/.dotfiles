filetype plugin indent on

set smartindent

call plug#begin()
Plug 'wakatime/vim-wakatime'      " Wakatime support
Plug 'afshinm/npm.vim' 		  " npm support
Plug 'tpope/vim-fugitive' 	  " git support
Plug 'mattn/emmet-vim' 		  " emmet html autocomplete 
Plug 'leafgarland/typescript-vim' " TypeScript syntax
Plug 'maxmellon/vim-jsx-pretty'   " JS and JSX syntax
Plug 'ycm-core/YouCompleteMe' 	  " Auto Completion Engine
Plug 'scrooloose/syntastic' 	  " Syntax Engine
Plug 'ervandew/supertab'	  " Auto Complete & Insert Lines with TAB
Plug 'vim-airline/vim-airline' 	  " Status Bar for VIM
Plug 'frazrepo/vim-rainbow' 	  " Rainbow Brackets
call plug#end()
